{
  "version": "7.0.0-alpha.2",
  "ignoreChanges": [
    "*.spec.*",
    "*.md"
  ],
  "command": {
    "publish": {
      "allowBranch": [
        "master",
        "next"
      ],
      "message": "Publish %s"
    },
    "bootstrap": {
      "npmClientArgs": [
        "--no-package-lock"
      ]
    }
  }
}
