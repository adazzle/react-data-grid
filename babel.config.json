{
  "presets": [
    ["@babel/env", {
      "loose": true,
      "bugfixes": true,
      "shippedProposals": true,
      "corejs": 3,
      "useBuiltIns": "entry"
    }],
    ["@babel/react", { "runtime": "automatic" }],
    "@babel/typescript",
    "@linaria"
  ],
  "plugins": [
    "@babel/transform-runtime",
    ["optimize-clsx", { "functionNames": ["getCellClassname"] }]
  ]
}
